{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "airbnb-typescript",
        "plugin:react/jsx-runtime"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "**/client/tsconfig.json"
    },
    "plugins": [
        "react",
        "react-refresh"
    ],
    "rules": {
        "react-refresh/only-export-components": "warn",
        "import/extensions": "off"
    }
}